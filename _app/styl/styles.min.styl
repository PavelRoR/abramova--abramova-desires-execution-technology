A='Akrobat', sans-serif
AB='AkrobatBold', sans-serif
C='Comfortaa', sans-serif
CB='ComfortaaBold', sans-serif
CL='ComfortaaLight', sans-serif

@font-face {
    font-family: 'Akrobat';
    src: url('../fonts/Akrobat.eot');
    src: url('../fonts/Akrobat.eot') format('embedded-opentype'),
         url('../fonts/Akrobat.woff2') format('woff2'),
         url('../fonts/Akrobat.woff') format('woff'),
         url('../fonts/Akrobat.ttf') format('truetype'),
         url('../fonts/Akrobat.svg#Akrobat') format('svg');
	font-display: fallback;
	font-weight: normal;
	font-style: normal;
}
@font-face {
    font-family: 'AkrobatBold';
    src: url('../fonts/AkrobatBold.eot');
    src: url('../fonts/AkrobatBold.eot') format('embedded-opentype'),
         url('../fonts/AkrobatBold.woff2') format('woff2'),
         url('../fonts/AkrobatBold.woff') format('woff'),
         url('../fonts/AkrobatBold.ttf') format('truetype'),
         url('../fonts/AkrobatBold.svg#AkrobatBold') format('svg');
	font-display: fallback;
	font-weight: bold
	font-style: normal;
}
